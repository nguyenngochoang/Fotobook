<div class="col-lg-10 col-12 us mid">
        <div class="row  us gocenter">
          <div id="col-lg-4 col-12 useravatar inline-block">
           <%= image_tag 'profile/4k-ultra-hd-abstract-nebula-space-creative-galaxy-background_nybzvmxi__F0000.png',alt:"Sample image",class:"us rounded-circle" %>
          </div>
          <div class="col-lg-8 col-12 inline-block ml-2 us userdiv">
            <a href="#" class="us edit-btn">Edit Profile</a>
            <p class="font-weight-bolder us username"><%="#{@user.first_name} #{@user.last_name}"%></p>
            <ul class="list-inline">
              <li class="list-inline-item"><a href="#" onClick="return false;" class="btn btn-link us usertab font-weight-bolder"><%=" #{get_all_photos(current_user).count} Photos"%></a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><a  href="#" onClick="return false;"  class="btn btn-link us usertab text-black-50"><%=" #{get_albums_count} Albums"%></a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><a  href="#" onClick="return false;" class="btn btn-link us usertab text-black-50">22 Followings</a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><a  href="#" onClick="return false;" class="btn btn-link us usertab text-black-50">13 Followers</a></li>
            </ul>
          </div>

        </div>
        <a href="#" class="us addPhoto-btn font-weight-bolder">Add Photo</a>

        <%= render partial: 'users/usergallery',locals: {mode:"photo",user:current_user} %>
        <%= render partial: 'users/usergallery',locals: {mode:"album",user:current_user} %>

