<div class="col-lg-10 col-12 us mid">
  <div class="row  us gocenter">
    <div id="col-lg-4 col-12 useravatar inline-block">
      <%= image_tag 'profile/4k-ultra-hd-abstract-nebula-space-creative-galaxy-background_nybzvmxi__F0000.png',alt:"Sample image",class:"us rounded-circle",'data-id':current_user.id %>
    </div>
    <div class="col-lg-8 col-12 inline-block ml-2 us userdiv">
      <a href="#" class="us edit-btn">Edit Profile</a>
      <p class="font-weight-bolder us username"><%="#{@user.first_name} #{@user.last_name}"%></p>
      <ul class="list-inline">
        <li class="list-inline-item"><a href="#" onClick="return false;" class="btn btn-link us usertab font-weight-bolder"><%=" #{get_all_photos(@user).size} Photos"%></a></li>
        <li class="list-inline-item">|</li>
        <li class="list-inline-item"><a  href="#" onClick="return false;"  class="btn btn-link us usertab text-black-50"><%=" #{get_albums_count(@user)} Albums"%></a></li>
        <li class="list-inline-item">|</li>
        <li class="list-inline-item"><a  href="#" onClick="return false;" class="btn btn-link us usertab text-black-50"><%="#{@user.followees.size} Followings "%></a></li>
        <li class="list-inline-item">|</li>
        <li class="list-inline-item"><a  href="#" onClick="return false;" class="btn btn-link us usertab text-black-50"><%="#{@user.followers.size} Followers"%></a></li>
      </ul>
    </div>
    <div class="profile-content">
      <%= render partial: 'users/userphoto',locals: {mode:"photo",user:current_user} %>
    </div>
  </div>
</div>




